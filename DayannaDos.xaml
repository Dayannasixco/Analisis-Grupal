<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AnalisisGrupal.DayannaDos"
             Title="DayannaDos">
    <VerticalStackLayout
     
     HorizontalOptions="Center"
     VerticalOptions="Center"
     Spacing="20">

        <Frame 
         BorderColor="Green" 
         CornerRadius="10" 
         Padding="30"
         HasShadow="True"
         HorizontalOptions="Center"
         VerticalOptions="Center">

            <Label
             x:Name="lblCuenta"
             Text="10"
             FontSize="72"
             FontAttributes="Bold"
             TextColor="Green"
             HorizontalOptions="Center"
             VerticalOptions="Center" />

        </Frame>

    </VerticalStackLayout>
</ContentPage>